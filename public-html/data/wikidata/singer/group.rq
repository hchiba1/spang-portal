SELECT *
WHERE {
    ?category wdt:P279* wd:Q2088357 . # subclass_of musical_ensemble
    ?group wdt:P31 ?category . # instace_of
    ?group wdt:P527 ?member .
    ?category rdfs:label ?category_label .
    ?group rdfs:label ?group_label .
    ?member rdfs:label ?member_label .
    FILTER (lang(?category_label) = "ja")
    FILTER (lang(?group_label) = "ja")
    FILTER (lang(?member_label) = "ja")
}
