# @title Basic information of cluster
# @param arg0=2000 

SELECT ?label ?description ?member_count ?page
WHERE {
    ?cluster a orth:OrthologGroup ;
             orth:inDataset mbgdr:default ;
             dct:identifier "$1";
             rdfs:label ?label ;
             dct:description ?description ;
             mbgd:memberCount ?member_count ;
             foaf:page ?page .
}

