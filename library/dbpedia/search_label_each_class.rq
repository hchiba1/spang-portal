# @title Free text search in Virtuoso (count instances of each class)
# @param arg0=okazaki 

SELECT ?class (COUNT(DISTINCT ?instance) AS ?count)
WHERE {
    ?instance rdfs:label ?text .
    ?text bif:contains '"$1"' .
    ?instance a ?class .
    ?class rdfs:subClassOf+ owl:Thing .
}
ORDER BY DESC(?count)

